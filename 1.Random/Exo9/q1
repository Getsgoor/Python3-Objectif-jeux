from random import randint

for i in range(100) :
    de = randint(1, 4)
    if (de == 1) :
        print(randint(1,6), end = " ")
    elif (de == 2) :
        print(randint(1,8), end = " ")
    elif (de == 3) :
        print(randint(1,12), end = " ")
    else :
        print(randint(1,20), end = " ")
print()
    