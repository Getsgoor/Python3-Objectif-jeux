
# Crible d'Eratosthène pour rechercher les nombres premiers de 1 à  n
from math import *

def crible_eratosthene(n) :
    liste = [True] * (n + 1)
    liste[0] = liste[1] = False
    for i in range(2, int(sqrt(n)) + 1) :
        if (liste[i]):
            for j in range(2 * i, n + 1, i):
                liste[j] = False
                
    premiers = [2]
    for i in range(3, n+1, 2):
        if (liste[i]):
            premiers.append(i)
    return premiers

print(crible_eratosthene(1000))
